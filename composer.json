{
  "name": "deadmantfa/yii2-rbac",
  "description": "Yii2 RBAC Module, updated for PHP 8 and above",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "rbac",
    "routes access",
    "rbac gui"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Alex Prokopenko",
      "email": "aprokopenko@justcoded.com"
    },
    {
      "name": "Wenceslaus Dsilva",
      "email": "wenceslausdsilva@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.2.0",
    "yiisoft/yii2": "^2.0.52",
    "kartik-v/yii2-widget-select2": "^2.2.0",
    "yiisoft/yii2-bootstrap4": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "deadmantfa\\yii2\\rbac\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "require-dev": {
    "rector/rector": "^2.0"
  }
}
